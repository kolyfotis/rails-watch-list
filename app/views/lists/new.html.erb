<div class="container">
  <h1>New List</h1>

  <%= simple_form_for(@list) do |f| %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :photo, as: :file %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
  <% end %>

  <%= link_to 'Back', lists_path %>
</div>
